//读取字符，返回位置
#include <stdio.h>
#include <string.h>
int location(char ch);
int main()
{
	char ch;
	//ch = 'd';
	while ((ch = getchar())!=EOF)
	{
		if (ch == '\n')
			continue;
		printf("%d\n",location(ch));
    
    //if语句确保当输入结束按enter键时，产生的‘\n’，不会作为字符来产生-1；
    //EOF的准确用法是当按下Ctrl+Z时，停止循环，当然！=-1也可以；
    
    
    
	}
	
	
	return 0;
}
int location(char ch)
{
	int ret;
	if (ch >= 'A' and ch <= 'Z')
		ret = ch - 'A' + 1;
	else if (ch >= 'a' and ch <= 'z')
		ret = ch - 'a' + 1;
	else 
		ret = -1;
	return ret;
}
